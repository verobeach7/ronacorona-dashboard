   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"data/time_confirmed.csv\")\n",
    "# .drop()을 이용해 일부 컬럼을 삭제할 수 있음\n",
    "df = (\n",
    "    df.drop([\"Province/State\", \"Country/Region\", \"Lat\", \"Long\"], axis=1)\n",
    "    .sum()\n",
    "    .reset_index(name=\"total\")\n",
    ")\n",
    "df = df.rename(columns={\"index\": \"date\"})\n",
    "df"
   ]
  }
